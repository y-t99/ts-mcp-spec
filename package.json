{
  "name": "ts-mcp-spec",
  "version": "0.1.0",
  "description": "TypeScript definition of Model Context Protocol specification",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "prepublishOnly": "pnpm run build",
    "fix": "pnpm run lint:fix && pnpm run format"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "llm",
    "ai",
    "typescript"
  ],
  "author": "itou ng",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "vitest": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/y-t99/ts-mcp-spec.git"
  },
  "bugs": {
    "url": "https://github.com/y-t99/ts-mcp-spec/issues"
  },
  "homepage": "https://github.com/y-t99/ts-mcp-spec#readme",
  "packageManager": "pnpm@9.15.3",
  "dependencies": {
    "zod": "^3.24.2"
  }
}
